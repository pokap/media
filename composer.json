{
    "name": "Pokap/media",
    "description": "Simply media loading meta-data per link.",
    "keywords": ["media","link","meta-data"],
    "type": "library",
    "licence": "MIT",
    "authors": [
        {
            "name": "Florent DENIS",
            "email": "florentdenisp@gmail.com",
            "homepage": "http://www.florentdenis.net"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "scottmac/opengraph",
                "version": "master",
                "source": {
                    "url": "https://github.com/scottmac/opengraph.git",
                    "type": "git",
                    "reference": "origin/master"
                }
            }
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "zendframework/zendframework": "*",
        "scottmac/opengraph": "*"
    },
    "autoload": {
        "psr-0": {
            "Pok\\Media": "",
            "OpenGraph": "vendor/scottmac/opengraph"
        }
    },
    "target-dir": "Pok/Media"
}
